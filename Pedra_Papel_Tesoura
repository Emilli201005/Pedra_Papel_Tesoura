import java.util.Scanner;

public class App {
public  static  void  main ( String [] args ) {
    String pedra = "1";
     String papel = "2";
     String tesoura = "3";
     String pedra2 = "4";
     String papel2 = "5";
     String tesoura2 = "6";



        Scanner teclado = new Scanner(System.in);
        System.out.print("Digite 1 para pedra, 2 para papel ou 3 para tesoura");
        String jogador1 = teclado.next();

        if (jogador1.equals(pedra)){
            System.out.print("Pedra");
        }
        String jogador2 = teclado.next();
         
         if (jogador2.equals(papel)){
            System.out.print("Papel");
        }
       
        String jogador3 = teclado.next();

   
           if (jogador3.equals(tesoura)){
            System.out.print("Tesoura");
        }
       
        Scanner teclado2 = new Scanner(System.in);
        System.out.print("Digite 1 para pedra, 2 para papel ou 3 para tesoura"); 
    
        String computador1 = teclado2.next();

        if (computador1.equals(pedra2)){
            System.out.print("Pedra");
        }
        String computador2 = teclado2.next();
         
         if (computador2.equals(papel2)){
            System.out.print("Papel");
        }
       
        String computador3 = teclado2.next();

   
           if (computador3.equals(tesoura2)){
            System.out.print("Tesoura");
        }
     
     
    


 

}
}
